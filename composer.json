{
    "name": "zainpay/wordpress-sdk",
    "description": "WordPress adapter for ZainPay PHP SDK",
    "type": "library",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Ibrahim Nasir",
            "email": "me@ibrahim.ng",
            "homepage": "https://ibrahim.ng"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/kh4l33l/zainpay-wordpress-sdk/"
        }
    ],
    "require": {
        "php": ">=7.4",
        "zainpay/sdk": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Zainpay\\SDK\\WordPress\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
